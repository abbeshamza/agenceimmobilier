{% extends '::base.html.twig' %}
{% block contenu%}
<form class="form-control" enctype="multipart/form-data" action="{{ path('upload') }}" method="post">

    <h1>Upload File</h1>

    <form action="#" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}

        <input type="submit" value="Upload Document" />
    </form>
</form>
